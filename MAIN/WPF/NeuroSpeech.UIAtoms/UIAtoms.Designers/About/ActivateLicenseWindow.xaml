<Window x:Class="NeuroSpeech.UIAtoms.Design.License.ActivateLicenseWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
        Title="Activate UIAtoms License"
        ResizeMode="NoResize"
        Width="500"
        Height="300">
    <Canvas>
        <Image Canvas.Left="10" Canvas.Top="10" Height="103" Source="/NeuroSpeech.UIAtoms.Designers;component/About/UIAtomsHeader.png" Width="463" />
        <Label Canvas.Left="30" Canvas.Top="138" Content="Email Address:" Height="28" Name="label1" />
        <Label Canvas.Left="20" Canvas.Top="167" Content="Activation Code:" Height="28" Name="label2" />
        <TextBox Canvas.Left="123" Canvas.Top="140" Height="23" Name="txtEmailAddress" Width="324" />
        <TextBox Canvas.Left="123" Canvas.Top="169" Height="23" Name="txtActivationCode" Width="324" />
        <Button Canvas.Left="123" Canvas.Top="198" Content="Activate" Height="23" Name="activateButton" Width="75" Click="activateButton_Click" />
    </Canvas>
</Window>
